{% macro add_indicator_links(request, location_type, location, page) %}
<div class="row nav-holder indicator_links">
    <ul class="nav nav-tabs indicator_tabs">
    {% if location %}
        <li class="indicator_list">
            <a href="{{ request.route_url('performance_indicators',traverse=(location.id,)) }}"
            {% if page == 'performance'%}
                    class="selected" >
            {% else %}
                    >
            {% endif %}
            {% trans %} Performance {% endtrans %}
            </a>
        </li>
        <li class="indicator_list">
            <a href="{{ request.route_url('impact_indicators', traverse=(location.id)) }}"
            {% if page == 'impact' %}
                    class="selected" >
            {% else %}
                    >
            {% endif %}
            {% trans %} Impact {% endtrans %}
            </a>
        </li>
    {% else %}
        <li class="indicator_list">
            <a href="{{ request.route_url('performance_indicators',traverse=()) }}"
            {% if page == 'performance'%}
                    class="selected" >
            {% else %}
                    >
            {% endif %}
            {% trans %} Performance {% endtrans %}
            </a>
        </li>
        <li class="indicator_list">
            <a href="{{ request.route_url('impact_indicators',traverse=()) }}"
            {% if page == 'impact' %}
                    class="selected" >
            {% else %}
                    >
            {% endif %}
            {% trans %} Impact {% endtrans %}
            </a>
        </li>
    {% endif %}
        <li class="indicator_list">
            <a href="#">{% trans %} Result Framework {% endtrans %}</a>
        </li>
    </ul>
</div>
{% endmacro %}
